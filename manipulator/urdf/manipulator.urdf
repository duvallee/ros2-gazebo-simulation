<?xml version="1.0" ?>
<robot name="manipulator" xmlns:xacro="http://ros.org/wiki/xacro">

  <link name="footprint_link" />

  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.6" radius="0.2"/>
      </geometry>
    </visual>
  </link>

  <joint name="base_joint" type="fixed">
    <parent link="footprint_link"/>
    <child link="base_link"/>
  </joint>

</robot>
